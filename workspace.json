{
  "version": 2,
  "projects": {
    "api-gateway": "apps/api-gateway",
    "api-journeys": "apps/api-journeys",
    "api-languages": "apps/api-languages",
    "api-media": {
      "root": "apps/api-media",
      "sourceRoot": "apps/api-media/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-media",
            "main": "apps/api-media/src/main.ts",
            "tsConfig": "apps/api-media/tsconfig.app.json",
            "assets": ["apps/api-media/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-media/src/environments/environment.ts",
                  "with": "apps/api-media/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-media:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/api-media/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-media"],
          "options": {
            "jestConfig": "apps/api-media/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "api-users": "apps/api-users",
    "api-videos": "apps/api-videos",
    "apollo-logging": "libs/apollo/logging",
    "journeys": "apps/journeys",
    "journeys-admin": "apps/journeys-admin",
    "journeys-admin-e2e": "apps/journeys-admin-e2e",
    "journeys-e2e": "apps/journeys-e2e",
    "journeys-ui": "libs/journeys/ui",
    "locales": {
      "root": "libs/locales",
      "sourceRoot": "libs/locales/src",
      "projectType": "library",
      "targets": {},
      "tags": []
    },
    "nest-database": "libs/nest/database",
    "nest-decorators": "libs/nest/decorators",
    "nest/gqlAuthGuard": "libs/nest/gqlAuthGuard",
    "nest/powerBi": "libs/nest/powerBi",
    "shared-storybook": "libs/shared/storybook",
    "shared-ui": "libs/shared/ui",
    "watch": "apps/watch",
    "watch-admin": "apps/watch-admin",
    "watch-admin-e2e": "apps/watch-admin-e2e",
    "watch-e2e": "apps/watch-e2e"
  }
}
